node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build output (will be generated during build)
dist/
build/
.vite/

# Testing
coverage/
.nyc_output/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Environment files (should be injected at runtime)
.env
.env.local
.env.development
.env.test
.env.production

# Documentation
README.md
CHANGELOG.md
*.md

# CI/CD
.github/
.gitlab-ci.yml
.travis.yml

# Misc
*.log
.cache/
